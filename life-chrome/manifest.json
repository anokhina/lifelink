  {
    "name": "Life links",
    "version": "1.1",
    "description": "Daily links",
    "permissions": [
      "storage",
      "activeTab",
      "tabs",
      "pageCapture",
      "contextMenus",
      "<all_urls>"
    ],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popups.html"
    },
    "icons": {
      "128": "icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
  }